<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="
            http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="01-alter-acm_disposition-for-decision" author="riste.tutureski">
        <addColumn tableName="acm_disposition">
            <column name="gms_award_value" type="NUMBER(32,0)"/>
        </addColumn>
    </changeSet>

    <changeSet id="02-alter-acm_disposition-for-decision" author="riste.tutureski">
        <addColumn tableName="acm_disposition">
            <column name="gms_rating" type="NUMBER(32,0)"/>
        </addColumn>
    </changeSet>

</databaseChangeLog>

