<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="
            http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd"
        logicalFilePath="/ddl/tables/award-db-tables-1.0.xml">

    <changeSet id="01-alter-acm_complaint-for-submission" author="riste.tutureski">
        <addColumn tableName="acm_complaint">
            <column name="gms_grant_id" type="${idType}"/>
        </addColumn>
    </changeSet>

    <changeSet id="02-alter-acm_complaint-for-submission" author="riste.tutureski">
        <addColumn tableName="acm_complaint">
            <column name="gms_rating" type="NUMBER(32,0)"/>
        </addColumn>
    </changeSet>

</databaseChangeLog>

