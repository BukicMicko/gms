<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="
            http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="01-alter-acm_case_file-for-grant" author="riste.tutureski">
        <addColumn tableName="acm_case_file">
            <column name="gms_total_anticipated_funds" type="NUMBER(32,0)"/>
            <column name="gms_expected_award_date" type="${timestampType}"/>
            <column name="gms_grant_type" type="VARCHAR(1000)"/>
        </addColumn>
    </changeSet>

</databaseChangeLog>

