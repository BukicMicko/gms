          "headerCellFilter": "translate"
        },
        {
          "name": "value",
          "displayName": "budget.comp.timesSummary.table.columns.value",
          "headerCellFilter": "translate"
        },
        {
          "name": "title",
          "displayName": "budget.comp.timesSummary.table.columns.title",
          "headerCellFilter": "translate"
        },
        {
          "name": "description",
          "displayName": "budget.comp.timesSummary.table.columns.description",
