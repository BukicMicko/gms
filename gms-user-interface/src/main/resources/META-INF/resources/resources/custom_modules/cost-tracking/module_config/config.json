      "title": "Person",
      "enabled": true,
      "type": "grid",
      "enableFiltering": true,
      "paginationPageSizes": [
        5,
        10,
        20,
        50
      ],
      "paginationPageSize": 5,
      "columnDefs": [
        {
          "name": "fullName",
          "displayName": "budget.comp.people.table.columns.fullName",
          "headerCellFilter": "translate"
        },
        {
          "name": "userId",
          "displayName": "budget.comp.people.table.columns.userId",
          "headerCellFilter": "translate"
        }
      ]
    },
    {
      "id": "time-summary",
      "title": "Time Summary",
